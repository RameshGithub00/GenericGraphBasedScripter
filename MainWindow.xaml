<Window x:Class="GenericGraphBasedScripter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenericGraphBasedScripter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="80">
            <Border CornerRadius="5" Padding=" 10 " BorderThickness="5" BorderBrush="BlueViolet">
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="Hello" FontSize="24"/>
                    <Button Content="Press Me" Background="Red" Padding="10"/>
                </StackPanel>
            </Border>
            <Border CornerRadius="5" Padding=" 10 " BorderThickness="5" BorderBrush="BlueViolet">
                <TextBlock Text="Hello" FontSize="24"/>
            </Border>
        </StackPanel>
    </Grid>
</Window>
